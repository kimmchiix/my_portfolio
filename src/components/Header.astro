                <path d="M20.4782 65.9701C7.97803 58.9701 -7.02173 36.9701 6.05358 17.1197C14.1756 4.78922 18.1449 39.6368 20.4782 65.9701C20.8115 40.1368 56.8573 -9.88242 74.0533 3.11959C94.5533 18.6197 44.1449 51.1368 20.4782 65.9701C42.9782 65.4701 107.553 94.6196 97.5533 110.62C89.119 124.115 27.4782 103.97 20.4782 65.9701C20.4782 90.9701 40.5766 121.738 22.0535 110.62C3.47827 99.4701 15.9781 69.9701 20.4782 65.9701Z" fill="#B10F37"/>
